PROCEDURE Dot_Product(vector1:ARRAY_OF double[], vector2 : ARRAY_OF double[])
VAR
    PS : integer =0;

BEGIN
    for(let i = 0; i < vector1.length; i++){
        PS += vector1[i] * vector2[i]
      }
 
  }
END 

